{
    "name": "beepnl/beep",
    "vendor":"BEEP Foundation",
    "version": "2.2.0",
    "description": "BEEP digital hive monitor",
    "keywords": ["Beep", "Beekeeping", "apiary", "hive", "app", "measurement system"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.4",
        "fruitcake/laravel-cors": "^2.0.4",
        "doctrine/dbal": "^2.9",
        "ellipsesynergie/api-response": "0.12.*",
        "fideloper/proxy": "^4.0",
        "fightbulc/moment": "*",
        "guzzlehttp/guzzle": "^6.5",
        "iatstuti/laravel-cascade-soft-deletes": "^2.0.0",
        "influxdb/influxdb-php": "1.*",
        "intervention/image": "^2.4.2",
        "kalnoy/nestedset": "^5.0.1",
        "laravel/framework": "^6.2",
        "laravel/tinker": "~2.0",
        "laravelcollective/html": "^6.1",
        "league/flysystem-aws-s3-v3": "~1.0",
        "league/flysystem-cached-adapter": "~1.0",
        "mcamara/laravel-localization": "1.5.*",
        "phpoffice/phpspreadsheet": "^1.2",
        "predis/predis": "^1.1",
        "trebol/entrust": "^0.6",
        "tray-labs/laravel-influxdb": "*"
    },
    "require-dev": {
        "mpociot/laravel-apidoc-generator": "^3.17",
        "appzcoder/crud-generator": "^3.0",
        "filp/whoops": "^2.0",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^8.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "platform":{"php": "7.4"}
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
